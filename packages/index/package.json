    "@ot-builder/cfg-log": "0.1.0",
    "@ot-builder/ft-metadata": "0.1.0",
    "@ot-builder/ft-name": "0.1.0",
    "@ot-builder/ft-glyphs": "0.1.0",
    "@ot-builder/ft-encoding": "0.1.0",
    "@ot-builder/ft-layout": "0.1.0",
    "@ot-builder/ft-sfnt": "0.1.0",
    "@ot-builder/font": "0.1.0",
    "@ot-builder/prelude": "0.1.0",
    "@ot-builder/primitive": "0.1.0",
