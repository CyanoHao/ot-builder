{
  "name": "@ot-builder/rectify",
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "jest": "^24.8.0",
    "@types/jest": "^24.0.15"
  },
  "scripts": {
    "build": "tsc -b ./tsconfig.package.json",
